{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@sac/table*": [
        "libs/table/src*",
        "libs/table/src/index.ts"
      ],
      "@sac/table/sticky": [
        "libs/table/sticky/src/index.ts"
      ],
      "@sac/table/detail-row": [
        "libs/table/detail-row/src/index.ts"
      ],
      "@sac/table/block-ui": [
        "libs/table/block-ui/src/index.ts"
      ],
      "@sac/table/mat-checkbox-column": [
        "libs/table/mat-checkbox-column/src/index.ts"
      ],
      "@sac/table/mat-paginator": [
        "libs/table/mat-paginator/src/index.ts"
      ],
      "@sac/table/mat-sort": [
        "libs/table/mat-sort/src/index.ts"
      ],
      "@sac/demo-apps/table-demo": [
        "libs/demo-apps/table-demo/src/index.ts"
      ],
      "@sac/demo-apps/shared": [
        "libs/demo-apps/shared/src/index.ts"
      ],
      "@sac/*": [
        "libs/*"
      ],
      "@sac/docsi": [
        "libs/docsi/src/index.ts"
      ],
      "@sac/docsi/webpack": [
        "libs/docsi/webpack/src/index.ts"
      ],
      "@sac/docsi/toc": [
        "libs/docsi/toc/src/index.ts"
      ],
      "@sac/docsi/material-suite": [
        "libs/docsi/material-suite/src/index.ts"
      ],
      "@sac/table/transpose": [
        "libs/table/transpose/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
