{
  "compileOnSave": false,
  "compilerOptions": {
    "importHelpers": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "module": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@pebula/apps/table/concepts": [
        "libs/apps/table/concepts/src/index.ts"
      ],
      "@pebula/apps/table/features": [
        "libs/apps/table/features/src/index.ts"
      ],
      "@pebula/apps/table/stories": [
        "libs/apps/table/stories/src/index.ts"
      ],
      "@pebula/apps/table/demos": [
        "libs/apps/table/demos/src/index.ts"
      ],
      "@pebula/apps/table/shared": [
        "libs/apps/table/shared/src/index.ts"
      ],
      "@pebula/docsi": [
        "libs/docsi/src/index.ts"
      ],
      "@pebula/docsi/webpack": [
        "libs/docsi/webpack/src/index.ts"
      ],
      "@pebula/docsi/toc": [
        "libs/docsi/toc/src/index.ts"
      ],
      "@pebula/docsi/material-suite": [
        "libs/docsi/material-suite/src/index.ts"
      ],
      "@pebula/table*": [
        "libs/table/src*",
        "libs/table/src/index.ts"
      ],
      "@pebula/table/sticky": [
        "libs/table/sticky/src/index.ts"
      ],
      "@pebula/table/detail-row": [
        "libs/table/detail-row/src/index.ts"
      ],
      "@pebula/table/block-ui": [
        "libs/table/block-ui/src/index.ts"
      ],
      "@pebula/table/transpose": [
        "libs/table/transpose/src/index.ts"
      ],
      "@pebula/table/target-events": [
        "libs/table/target-events/src/index.ts"
      ],
      "@pebula/table/material/selection-column": [
        "libs/table/material/selection-column/src/index.ts"
      ],
      "@pebula/table/material/paginator": [
        "libs/table/material/paginator/src/index.ts"
      ],
      "@pebula/table/material/sort": [
        "libs/table/material/sort/src/index.ts"
      ],
      "@pebula/table/material/cell-tooltip": [
        "libs/table/material/cell-tooltip/src/index.ts"
      ],
      "@pebula/table/material": [
        "libs/table/material/src/index.ts"
      ],
      "@pebula/table/drag": [
        "libs/table/drag/src/index.ts"
      ],
      "@pebula/utils": [
        "libs/utils/src/index.ts"
      ],
      "@pebula/*": [
        "libs/*"
      ]
    }
  },
  "exclude": [
    "dist",
    "node_modules",
    "tmp"
  ]
}
