{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@neg/demo-apps/concepts": [
        "libs/demo-apps/concepts/src/index.ts"
      ],
      "@neg/demo-apps/features": [
        "libs/demo-apps/features/src/index.ts"
      ],
      "@neg/demo-apps/stories": [
        "libs/demo-apps/stories/src/index.ts"
      ],
      "@neg/demo-apps/demos": [
        "libs/demo-apps/demos/src/index.ts"
      ],
      "@neg/demo-apps/shared": [
        "libs/demo-apps/shared/src/index.ts"
      ],
      "@neg/docsi": [
        "libs/docsi/src/index.ts"
      ],
      "@neg/docsi/webpack": [
        "libs/docsi/webpack/src/index.ts"
      ],
      "@neg/docsi/toc": [
        "libs/docsi/toc/src/index.ts"
      ],
      "@neg/docsi/material-suite": [
        "libs/docsi/material-suite/src/index.ts"
      ],
      "@neg/table*": [
        "libs/table/src*",
        "libs/table/src/index.ts"
      ],
      "@neg/table/sticky": [
        "libs/table/sticky/src/index.ts"
      ],
      "@neg/table/detail-row": [
        "libs/table/detail-row/src/index.ts"
      ],
      "@neg/table/block-ui": [
        "libs/table/block-ui/src/index.ts"
      ],
      "@neg/table/transpose": [
        "libs/table/transpose/src/index.ts"
      ],
      "@neg/table/target-events": [
        "libs/table/target-events/src/index.ts"
      ],
      "@neg/table/material/selection-column": [
        "libs/table/material/selection-column/src/index.ts"
      ],
      "@neg/table/material/paginator": [
        "libs/table/material/paginator/src/index.ts"
      ],
      "@neg/table/material/sort": [
        "libs/table/material/sort/src/index.ts"
      ],
      "@neg/table/material/cell-tooltip": [
        "libs/table/material/cell-tooltip/src/index.ts"
      ],
      "@neg/table/material": [
        "libs/table/material/src/index.ts"
      ],
      "@neg/table/drag": [
        "libs/table/drag/src/index.ts"
      ],
      "@neg/*": [
        "libs/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
